<view bindtap="bindViewTap" class="logo">
  <image class="logo-avatar" mode='widthFix' src="../../images/logo.jpg" background-size="cover"></image>
</view>


<view wx:for="{{list}}">
<view class="weui-cell weui-cell_access" bindtap='click' data-gusetName="{{item-gusetName}}" data-model='{{item.model}}'>
  <view class="weui-cell__bd">
   <view style="display: inline-block; vertical-align: middle">订单号:</view>
    <view class="weui-badge" style="margin-left: 5px;font-size:15px">{{item.model}}</view>
   
  </view>
  <view class="weui-cell__ft weui-cell__ft_in-access">客户名称:{{item.guestName}}</view>
</view>
</view>